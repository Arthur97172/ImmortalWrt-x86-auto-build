	option address_strategy 'ipv4_only'
	option resolve_strategy 'ipv4_only'
	option outbound 'direct-out'

config dns_server 'proxy_dns'
	option label 'proxyDns'
	option enabled '1'
	option address 'tls://8.8.8.8'
	option address_resolver 'local_dns'
	option address_strategy 'ipv4_only'
	option resolve_strategy 'ipv4_only'
	option outbound 'connection_label'

config dns_rule '001_domain'
	option label 'geosite-cn'
	option enabled '1'
	option mode 'default'
	list rule_set 'cn-geosite-rules'
	option server 'local_dns'
	option dns_disable_cache '1'

config dns_rule '002_domain'
	option label 'clash mode:direct'
	option enabled '1'
	option mode 'default'
	option clash_mode 'direct'
	option server 'local_dns'
	option dns_disable_cache '1'

config dns_rule '003_domain'
	option label 'clash_mode:global'
	option enabled '1'
	option mode 'default'
	option clash_mode 'global'
	option server 'proxy_dns'
	option dns_disable_cache '1'

config dns_rule '004_domain'
	option label 'geosite-geolocation-!cn'
	option enabled '1'
	option mode 'default'
	list rule_set 'cn-geolocation-geosite-rules'
	option server 'proxy_dns'
	option dns_disable_cache '1'

