config dns_server 'dns_40002'
	option label 'proxyDns'
	option enabled '1'
	option address 'https://dns.google/dns-query'
	option address_resolver 'dns_40001'
	option address_strategy 'ipv4_only'
	option resolve_strategy 'ipv4_only'
	option outbound '60001'
	option client_subnet '1.0.1.0'

config dns_rule '50001_domain'
	option label 'geosite-cn'
	option enabled '1'
	option mode 'default'
	list rule_set '80017'
	option server 'dns_40001'
	option dns_disable_cache '1'

config dns_rule '50002_domain'
	option label 'clash mode:direct'
	option enabled '1'
	option mode 'default'
	option clash_mode 'direct'
	option server 'dns_40001'
	option dns_disable_cache '1'

config dns_rule '50003_domain'
	option label 'clash_mode:global'
	option enabled '1'
	option mode 'default'
	option clash_mode 'global'
	option server 'dns_40002'
	option dns_disable_cache '1'

config dns_rule '50004_domain'
	option label 'geosite-geolocation-!cn'
	option enabled '1'
	option mode 'default'
	list rule_set '80015'
	option server 'dns_40002'
	option dns_disable_cache '1'
